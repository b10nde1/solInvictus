{
    "name": "Sol Invictus",
    "version": "1.1",
    "description": "Sol Invictus return list of urls in sitemap.xml + error pages",
    "permissions":["tabCapture","tabs","activeTab","declarativeContent","storage"],
    "background":{
        "scripts":["background.js"],
        "presistent": false
    },
    "page_action":{
        "default_popup":"popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
          }
    },
    "options_page":"options.html",
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
    "manifest_version": 2
  }
